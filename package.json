  "main": "index.js",
  "scripts": {
    "start": "nodemon --delay 2.5 -w dist dist/server.js",
    "complie-server": "babel server --presets es2015-node --out-dir dist --source-maps ",
    "watch-server": "babel --watch server --presets es2015-node --out-dir dist --source-maps ",
    "start-hook": "nodemon -w server hook/start_hook.js",
    "complie": "webpack -w",
    "watch": "webpack-dev-server --hot --inline",
    "eslint": "eslint --ext jsx,js, src server webpack.config.js",
    "dev-all": " npm run watch-server & npm run watch",
    "dev-all-hook": "npm run watch echo \"npm run start-hook fail\" & npm run start-hook  ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "模块"
  ],
