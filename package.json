    "express": "^4.14.0",
    "iconv-lite": "^0.4.24",
    "mongodb": "^2.2.6",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "request": "^2.87.0",
    "source-map-support": "^0.5.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.9.0",
